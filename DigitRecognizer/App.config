<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="Width" value="28" /> <!-- Width of each training image -->
    <add key="Height" value="28" /> <!-- Height of each training image -->
    <add key="ClassCount" value="10" /> <!-- Number of unique classes -->
    <add key="TrainCount" value="0" /> <!-- Max number of rows to read from the training set (useful for preparing learning curves). Set to 0 for all data. -->
    <add key="Sigma" value="1" /> <!-- SVM sigma training value, smoothes the gaussian ie., fuzziness factor (lower values = lower bias and higher variance and maybe overfitting, higher values = higher bias and lower variance). -->
    <add key="TrainPath" value="../../../data/train.csv" /> <!-- Path to training csv file -->
    <add key="CvPath" value="../../../data/cv.csv" /> <!-- Path to CV csv file -->
    <add key="TestPath" value="../../../data/test.csv" /> <!-- Path to test csv file -->
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Accord" publicKeyToken="fa1a88e29555ccf7" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.8.0.0" newVersion="3.8.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Accord.Math" publicKeyToken="fa1a88e29555ccf7" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.8.0.0" newVersion="3.8.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
